		event = { time = 0 sound = { soundeffect = super_heavy_battleship_damaged_defend }}
		event = { time = 0 sound = { soundeffect = super_heavy_battleship_attacked }}

		game_data = {
			hitmiss_effect = { time = 0.3 type = big }
			hitmiss_effect = { time = 0.35 type = big }
